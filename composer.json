{
    "name": "tomazahlin/symfony-core-bundle",
    "type": "symfony-bundle",
    "description": "Core bundle which provides a lot usable classes when building Symfony2 application with Doctrine2 ORM.",
    "keywords": ["symfony2", "core", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomaz Ahlin",
            "email": "tomaz.ahlin2@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.6.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0,>=3.0.12",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "tomazahlin/data-fixtures": "~1.0"

    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "matthiasnoback/symfony-config-test": "0.*",
        "matthiasnoback/symfony-dependency-injection-test": "0.*"
    },
    "autoload": {
      "psr-0": { "": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
